// See https://github.com/espressif/vscode-esp-idf-extension/blob/b90c27ccb9bfc0d2f76ec91fd0517d75fee9ebfd/docs/C_CPP_CONFIGURATION.md
{
    "configurations": [
      {
        "name": "ESP-IDF",
        "compilerPath": "${HOME}/.espressif/tools/xtensa-esp-elf/esp-13.2.0_20240530/xtensa-esp-elf/bin/xtensa-esp32s3-elf-gcc",
        "compileCommands": "${workspaceFolder}/build/compile_commands.json",
        "includePath": [
          "${config:idf.espIdfPath}/components/**",
          "${workspaceFolder}/**"
        ],
        "browse": {
          "path": [
            "${config:idf.espIdfPath}/components",
            "${workspaceFolder}"
          ]
        }
      }
    ],
    "version": 4
  }
